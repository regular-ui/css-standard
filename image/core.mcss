// Component:       `u-img`
// ========================================================================

.u-img {}



// Circle:
// ========================================================================

.u-img.u-img-circle {
    $border-radius: 1000px;
}



// Size Extends:
// ========================================================================

$u-img-extend_size = ($name, $width) {
    .u-img-#{$name} {
        width: $width;
    }
}
